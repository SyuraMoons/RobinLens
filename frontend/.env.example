# RobinLens - copy to .env and fill in your keys
# Only VITE_* variables are exposed to the browser.

# --- OpenAI (AI analysis, recommendations, support chatbot) ---
VITE_OPENAI_API_KEY=              # Optional: falls back to demo analysis
VITE_OPENAI_MODEL=gpt-4o          # Model for all AI features

# --- Chain & Router ---
VITE_CHAIN=                       # "sepolia" for Base Sepolia testnet, omit for mainnet
VITE_ROUTER_ADDRESS=              # RobinLensRouter address; direct curve calls when unset

# --- CoinGecko (AI Recommendations "Market" source) ---
VITE_COINGECKO_API_KEY=           # Optional: enables market data source
VITE_COINGECKO_BASE_URL=          # Optional: endpoint override
VITE_COINGECKO_PROXY=             # Optional: CORS proxy for production

# --- News API (AI Recommendations "News" source) ---
VITE_NEWS_API_KEY=                # Optional: enables news data source
VITE_NEWS_API_PROXY=              # Optional: CORS proxy for production
